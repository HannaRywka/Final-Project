<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="musicalgameapplication.GameController">
   <children>
      <Label fx:id="labelGame" layoutX="286.0" layoutY="14.0" text="Game" />
      <ImageView fx:id="imgAnimal" fitHeight="150.0" fitWidth="200.0" layoutX="267.0" layoutY="173.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@cow.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="imgBalloon" fitHeight="72.0" fitWidth="187.0" layoutX="377.0" layoutY="101.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@balloon.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgMusicalNote" fitHeight="150.0" fitWidth="200.0" layoutX="54.0" layoutY="125.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@semibreve.png" />
         </image>
      </ImageView>
      <ComboBox fx:id="cbMenu" layoutX="38.0" layoutY="32.0" prefWidth="150.0" promptText="Menu" />
      <Label fx:id="labelAnimalSpeak" layoutX="401.0" layoutY="116.0" text="?" textAlignment="CENTER" />
      <Label fx:id="labelTimer" layoutX="99.0" layoutY="92.0" text="Click on the note to start" />
      <Button fx:id="btnHold" layoutX="113.0" layoutY="297.0" mnemonicParsing="false" text="Hold" />
      <ImageView fx:id="imgMusicalScale" fitHeight="36.0" fitWidth="200.0" layoutX="55.0" layoutY="336.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
